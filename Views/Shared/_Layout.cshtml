<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi prueba ASP.NET</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/bundles/Plugin/css")
   
</head>
<body>
    

    <div class="container-fluid">
        @Html.Partial("_Notifications")
        <img src="@Url.Content("~/Images/logo_head.png")" alt="Logo" style="margin:auto" />
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Cupones Krispy Kreme ASP.NET</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/Plugin/js")

    @RenderSection("scripts", required: false)
</body>
</html>
